{
  "name": "webpack3-vue-cli",
  "version": "1.0.0",
  "description": "webpack3",
  "main": "index.js",
  "scripts": {
    "dll": "cross-env NODE_ENV=development webpack --config webpack/dll.config.js",
    "cp-comp": "sh bin/cp-script-components.sh",
    "dev": "npm run cp-comp && cross-env NODE_ENV=development webpack-dev-server --host 0.0.0.0 --config webpack/dev.config.js --progress --colors",
    "start": "sh bin/dev.sh",
    "mock": "npm run cp-comp && cross-env NODE_ENV=development  mock=true webpack-dev-server --host 0.0.0.0 --config webpack/dev.config.js --progress --colors",
    "start-mock": "sh bin/dev.mock.sh",
    "pretest": "sh bin/clean-public.sh && sh bin/cp-script-components.sh",
    "test": "cross-env NODE_ENV=test webpack --config webpack/test.config.js --progress --colors --hide-modules --display-error-details",
    "preprod": "sh bin/clean-public.sh && sh bin/uglify-script-components.sh",
    "prod": "cross-env NODE_ENV=production webpack --config webpack/prod.config.js --progress --colors --hide-modules --display-error-details",
    "uglify": "webpack --config ./src/script_components/uglify.config.js"
  },
  "keywords": [
    "webpack3"
  ],
  "author": "huangcj",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.1",
    "mint-ui": "^2.2.13",
    "rxjs": "^6.2.2",
    "vue": "2.5.8",
    "vue-router": "^3.0.1",
    "vue-rx": "^6.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.18.3",
    "chokidar": "^2.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "glob": "^7.1.2",
    "happypack": "^5.0.0",
    "html-webpack-include-assets-plugin": "^1.0.4",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^2.30.1",
    "imagemin-webpack-plugin": "^2.1.5",
    "mockjs": "^1.0.1-beta3",
    "node-notifier": "^5.2.1",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "path-to-regexp": "^2.2.1",
    "postcss-aspect-ratio-mini": "0.0.2",
    "postcss-auto-set-imageset": "0.0.4",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-px-to-viewport": "0.0.3",
    "postcss-write-svg": "^3.0.1",
    "prettier": "1.12.1",
    "sass-loader": "^7.0.2",
    "sass-resources-loader": "^1.3.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vconsole-webpack-plugin": "^1.4.0",
    "vue-loader": "^13.5.0",
    "vue-server-renderer": "^2.5.8",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "2.5.8",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.3",
    "webpack-uglify-parallel": "^0.1.4",
    "write-file-webpack-plugin": "^4.3.2"
  }
}
