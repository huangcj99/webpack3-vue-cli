{
  "name": "webpack3-vue-cli",
  "version": "1.0.0",
  "author": "joe<smallcatcat.joe@gmail.com>",
  "description": "webpack3 vue",
  "main": "index.js",
  "scripts": {
    "dll": "webpack --config webpack/dll.config.js",
    "predev": "sh bin/cp-script-components.sh",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --host 0.0.0.0 --watch-poll --config webpack/dev.config.js --progress --colors --inline",
    "start": "sh bin/dev.sh",
    "pretest": "sh bin/clean-build.sh && sh bin/cp-script-components.sh",
    "test": "cross-env NODE_ENV=test webpack --config webpack/test.config.js --progress --colors --hide-modules --display-error-details",
    "uglify": "webpack --config ./src/script_components/uglify.config.js",
    "preprod": "sh bin/clean-build.sh && sh bin/uglify-script-components.sh",
    "prod": "cross-env NODE_ENV=production webpack --optimize-minimize --config webpack/prod.config.js --progress --colors --hide-modules --display-error-details",
    "lint": "eslint --ext .js,.vue src"
  },
  "browserslist": [
    "> 5%",
    "last 4 versions"
  ],
  "dependencies": {
    "axios": "0.17.1",
    "mint-ui": "^2.2.13",
    "vue": "2.5.8"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-component": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "glob": "^7.1.2",
    "html-webpack-include-assets-plugin": "^1.0.2",
    "html-webpack-inline-source-plugin": "0.0.9",
    "html-webpack-plugin": "^2.30.1",
    "imagemin-webpack-plugin": "^1.5.2",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.9",
    "postcss-px-to-viewport": "0.0.3",
    "postcss-px2rem": "^0.3.0",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.1",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "url-loader": "^0.6.2",
    "vconsole-webpack-plugin": "^1.1.3",
    "vue-loader": "13.5.0",
    "vue-template-compiler": "2.5.8",
    "webpack": "^3.8.1",
    "webpack-dev-server": "~2.7.1",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-module-hot-accept": "^1.0.5",
    "webpack-uglify-parallel": "^0.1.4",
    "write-file-webpack-plugin": "^4.2.0"
  },
  "license": "ISC"
}
